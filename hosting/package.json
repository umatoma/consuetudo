    },
    "dependencies": {
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.9.4",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "sass": "^1.25.0",
        "typescript": "^3.7.5"
    },
    "devDependencies": {
        "@types/enzyme": "^3.10.4",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^24.9.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "jest": "^24.9.0",
        "parcel-bundler": "^1.12.4",
        "ts-jest": "^24.3.0"
    },
    "jest": {
        "preset": "ts-jest",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "setupFilesAfterEnv": [
            "./src/setUpTest.ts"
        ],
        "testMatch": [
            "**/?(*.)(spec|test).ts?(x)"
        ]
